{
    "name": "your-name-here/dub-manual",
    "description": "DubManual plugin for baserCMS",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "5.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "DubManual\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DubManual\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
